{
	"extends": ["react-app", "prettier", "prettier/react"],
	"plugins": ["jsx-a11y", "prettier"],
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"prettier/prettier": [
			"error",
			{
				"semi": true
			}
		]
	},
	"env": {
		"es6": true,
		"node": true
	}
}
